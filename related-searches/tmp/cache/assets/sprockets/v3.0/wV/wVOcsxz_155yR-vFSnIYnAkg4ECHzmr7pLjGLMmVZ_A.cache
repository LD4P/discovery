I"Õ(function() {
  var pagination;

  $.fn.scrollBottom = function() {
    return $(document).height() - this.scrollTop() - this.height();
  };

  pagination = {
    onLoad: function() {
      this.initObjects();
      if (this.sticky.length) {
        return this.bindEventListeners();
      }
    },
    initObjects: function() {
      return this.sticky = $('#sticky');
    },
    bindEventListeners: function() {
      return $(window).scroll(function() {
        var diff, footerHeight, windowBottom;
        windowBottom = $(window).scrollBottom();
        footerHeight = $('footer').height();
        if (footerHeight > windowBottom) {
          diff = footerHeight - windowBottom;
          return pagination.sticky.css({
            bottom: diff
          });
        } else {
          return pagination.sticky.css({
            bottom: '0'
          });
        }
      });
    }
  };

  $(document).ready(function() {
    return pagination.onLoad();
  });

}).call(this);
