query=PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> PREFIX bf: <http://id.loc.gov/ontologies/bibframe/> SELECT  DISTINCT ?opus WHERE { ?opus rdf:type <http://share-vde.org/rdfBibframe/Opus> . ?opus bf:hasExpression ?work1 . ?work1 bf:hasInstance ?instance1 .?instance1 bf:identifiedBy ?id1 .  ?id1 rdf:type bf:Lccn . ?opus bf:hasExpression ?work2 . ?work2 bf:hasInstance ?instance2 . ?instance2 bf:identifiedBy ?id2 .  ?id2 rdf:type bf:Lccn . FILTER(str(?work1) != str(?work2))}